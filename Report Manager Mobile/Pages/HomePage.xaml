<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="Report_Manager_Mobile.Pages.HomePage"
            xmlns:local="clr-namespace:Report_Manager_Mobile.Pages"
            xmlns:lang="clr-namespace:Report_Manager_Mobile.Resources.Languages"
            Title="HomePage"
            BarBackground="#0073B8"
            BarTextColor="White">


    <local:ServicesPage Title="{x:Static lang:AppResource.TabPageHeaderAvaliable}" IconImageSource="home.png"/>
    <local:Completed Title="{x:Static lang:AppResource.TabPageHeaderCompleted}" IconImageSource="check.png"/>
    <local:Settings Title="{x:Static lang:AppResource.TabPageHeaderSettings}" IconImageSource="settings.png" />

</TabbedPage>